{
  "name": "financ.io application",
  "version": "0.1.0",
  "author": "Ilya Petrushenko <ilya.petrushenko@yandex.ru>",
  "licence": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development && nodemon ./app.js",
    "start": "npm run nodemon",
    "nodemon": "nodemon --exec npm run dev",
    "prepush": "npm test",
    "production": "cross-env NODE_ENV=production && node ./app.js",
    "test": "cross-env NODE_ENV=test && nyc ava -s",
    "test-ci": "nyc ava --no-color"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "babel-polyfill": "^6.23.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.2",
    "chalk": "^2.3.2",
    "compression": "^1.7.2",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "handlebars": "^4.0.11",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^7.4.1",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nyc": "^11.0.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.5",
    "sequelize-cli": "^4.0.0",
    "static-favicon": "^2.0.0-alpha",
    "supertest": "^3.0.0",
    "validator": "^10.3.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "cross-env": "^5.1.1",
    "husky": "^0.13.4",
    "shx": "^0.2.2",
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "supertest": "^3.0.0"
  }
}
